{
  "cleanUrls": true,
  "trailingSlash": true,
  "redirects": [
    { "source": "/communities/palm-jumeirah/:path*", "destination": "/areas/palm-jumeirah/", "permanent": true },
    { "source": "/communities/emirates-hills/:path*", "destination": "/areas/emirates-hills/", "permanent": true },
    { "source": "/communities/arabian-ranches/:path*", "destination": "/areas/arabian-ranches/", "permanent": true },
    { "source": "/communities/dubai-hills/:path*", "destination": "/areas/dubai-hills/", "permanent": true },
    { "source": "/communities/:path*", "destination": "/areas/", "permanent": true }
  ],
  "headers": [
    {
      "source": "/images/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    },
    {
      "source": "/(.*).js",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=86400" }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Frame-Options", "value": "DENY" },
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "Referrer-Policy", "value": "strict-origin-when-cross-origin" }
      ]
    }
  ]
}
